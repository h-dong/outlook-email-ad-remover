{
    "name": "Outlook email ad remover",
    "version": "1.0.0",
    "description": "Removes the ad section of Microsoft's Outlook web mail client",
    "manifest_version": 2,
    "icons": {
      "16": "icons/16x16.png",
      "32": "icons/32x32.png",
      "48": "icons/48x48.png",
      "128": "icons/128x128.png"
    },
    "browser_action": {
      "default_icon": "icons/128x128.png"
    },
    "content_scripts": [
      {
        "matches": ["*://outlook.live.com/*"],
        "run_at": "document_end",
        "js": ["contentScript.js"]
      }
    ],
    "optional_permissions": [
      "*://outlook.live.com/*"
    ]
  }
