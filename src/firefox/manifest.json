{
  "name": "Email ad remover",
  "version": "1.0.2",
  "description": "Removes the ad section of Microsoft's Outlook web mail client",
  "manifest_version": 2,
  "icons": {
    "48": "icons/48x48.png",
    "96": "icons/96x96.png"
  },
  "content_scripts": [
    {
      "matches": ["*://outlook.live.com/*"],
      "run_at": "document_end",
      "js": ["contentScript.js"]
    }
  ],
  "optional_permissions": [
    "*://outlook.live.com/*"
  ]
}
